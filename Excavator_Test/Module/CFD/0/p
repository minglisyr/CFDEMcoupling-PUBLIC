FoamFile
{
    version 2.0;
    format ascii;
    class volScalarField;
    location "0";
    object p;
}
dimensions [ 0 2 -2 0 0 0 0 ];
internalField uniform 0.0;
boundaryField
{
    excavator
    {
        type zeroGradient;
    }

    outlet
    {
        type zeroGradient;
    }

    inlet
    {
        type totalPressure;
        p0 uniform 0;
        value uniform 0;
        rho none;
        psi none;
        U U;
        phi phi;
        gamma 1.0;
    }

    box
    {
        type zeroGradient;
    }

}
