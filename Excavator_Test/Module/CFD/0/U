FoamFile
{
    version 2.0;
    format ascii;
    class volVectorField;
    location "0";
    object U;
}
dimensions [ 0 1 -1 0 0 0 0 ];
internalField uniform (0.0 0.0 0.0);
boundaryField
{
    excavator
    {
        type fixedValue;
        value uniform ( 0 0 0);
    }

    outlet
    {
        type flowRateInletVelocity;
        value uniform ( 0.0 0.0 0.0 );
        volumetricFlowRate constant -1;
    }

    inlet
    {
        type pressureInletOutletVelocity;
        value uniform ( 0 0 0);
    }

    box
    {
        type fixedValue;
        value uniform ( 0 0 0);
    }

}
